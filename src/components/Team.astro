---
---

<section class="py-20 bg-navy" itemscope itemtype="https://schema.org/Organization">
    <div class="w-full">
        <div class="max-w-7xl mx-auto px-8 sm:px-12 lg:px-16">
            <!-- Section Header -->
            <div class="text-center mb-16">
                <h2 class="text-4xl md:text-5xl font-bold text-gold mb-6 font-playfair">
                    Meet The Dream Team
                </h2>
                <p class="text-offwhite text-lg leading-relaxed max-w-2xl mx-auto">
                    Our award-winning team of real estate professionals dedicated to delivering exceptional results for every client
                </p>
            </div>

            <!-- Team Grid - First Row -->
            <div class="grid grid-cols-1 md:grid-cols-3 gap-8 mb-12">
                <!-- Team Member 1 -->
                <div class="team-member-card p-8 text-center">
                    <div class="mb-6">
                        <div class="w-48 h-48 mx-auto overflow-hidden bg-gray-800">
                            <img 
                                src="/images/christian-fuentes-realtor-1.png" 
                                alt="Fernando Fuentes - Co-Owner & Operations Manager"
                                class="w-full h-full object-cover"
                            />
                        </div>
                    </div>
                    <h3 class="text-xl font-bold text-gold mb-2 font-playfair">Fernando Fuentes</h3>
                    <p class="text-offwhite/80 text-sm uppercase tracking-wider mb-3">Co-Owner & Operations Manager</p>
                    <p class="text-offwhite text-sm leading-relaxed mb-4">
                        Managing operations across three RE/MAX offices with 12+ years of experience in luxury real estate administration.
                    </p>
                    <div class="flex justify-center space-x-2">
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Operations</span>
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Management</span>
                    </div>
                </div>

                <!-- Team Member 2 -->
                <div class="team-member-card p-8 text-center">
                    <div class="mb-6">
                        <div class="w-48 h-48 mx-auto overflow-hidden bg-gray-800">
                            <img 
                                src="/images/christian-fuentes-realtor-1.png" 
                                alt="Maria Rodriguez - Senior Sales Associate"
                                class="w-full h-full object-cover"
                            />
                        </div>
                    </div>
                    <h3 class="text-xl font-bold text-gold mb-2 font-playfair">Fernando Fuentes</h3>
                    <p class="text-offwhite/80 text-sm uppercase tracking-wider mb-3">Senior Sales Associate</p>
                    <p class="text-offwhite text-sm leading-relaxed mb-4">
                        Specializing in luxury residential sales with expertise in Beverly Hills and Manhattan Beach markets.
                    </p>
                    <div class="flex justify-center space-x-2">
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Luxury Sales</span>
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Beverly Hills</span>
                    </div>
                </div>

                <!-- Team Member 3 -->
                <div class="team-member-card p-8 text-center">
                    <div class="mb-6">
                        <div class="w-48 h-48 mx-auto overflow-hidden bg-gray-800">
                            <img 
                                src="/images/christian-fuentes-realtor-1.png" 
                                alt="David Lee - Investment Specialist"
                                class="w-full h-full object-cover"
                            />
                        </div>
                    </div>
                    <h3 class="text-xl font-bold text-gold mb-2 font-playfair">Fernando Fuentes</h3>
                    <p class="text-offwhite/80 text-sm uppercase tracking-wider mb-3">Investment Specialist</p>
                    <p class="text-offwhite text-sm leading-relaxed mb-4">
                        Expert in investment properties and asset disposition with 8+ years in property rehabilitation and flipping.
                    </p>
                    <div class="flex justify-center space-x-2">
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Investments</span>
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Flipping</span>
                    </div>
                </div>
            </div>

            <!-- Team Grid - Second Row -->
            <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
                <!-- Team Member 4 -->
                <div class="team-member-card p-8 text-center">
                    <div class="mb-6">
                        <div class="w-48 h-48 mx-auto overflow-hidden bg-gray-800">
                            <img 
                                src="/images/christian-fuentes-realtor-1.png" 
                                alt="Sarah Chen - Marketing Director"
                                class="w-full h-full object-cover"
                            />
                        </div>
                    </div>
                    <h3 class="text-xl font-bold text-gold mb-2 font-playfair">Fernando Fuentes</h3>
                    <p class="text-offwhite/80 text-sm uppercase tracking-wider mb-3">Marketing Director</p>
                    <p class="text-offwhite text-sm leading-relaxed mb-4">

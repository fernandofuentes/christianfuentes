                <!-- Team Member 9 -->
                <div class="team-member-card p-8 text-center">
                    <div class="mb-6">
                        <div class="w-48 h-48 mx-auto overflow-hidden bg-gray-800">
                            <img 
                                src="/images/christian-fuentes-realtor-1.png" 
                                alt="James Wilson - Commercial Specialist"
                                class="w-full h-full object-cover"
                            />
                        </div>
                    </div>
                    <h3 class="text-xl font-bold text-gold mb-2 font-playfair">Fernando Fuentes</h3>
                    <p class="text-offwhite/80 text-sm uppercase tracking-wider mb-3">Commercial Specialist</p>
                    <p class="text-offwhite text-sm leading-relaxed mb-4">
                        Specializing in commercial real estate and multi-family investment properties across Chino Hills, CA.
                    </p>
                    <div class="flex justify-center space-x-2">
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Commercial</span>
                        <span class="px-3 py-1 bg-gold/20 text-gold text-xs rounded-full">Multi-Family</span>
                    </div>
                </div>
            </div>
            <!-- Team CTA -->
            <div class="text-center mt-16">
                <p class="text-offwhite text-lg mb-8 max-w-2xl mx-auto">
                    Ready to work with Chino Hills, CA premier real estate team? Let's make your real estate dreams a reality.
                </p>
                <a href="/consultation" class="btn-primary-yellow" aria-label="Contact the Christian Fuentes Dream Team">
                    <span>Work With Our Team</span>
                </a>
            </div>
        </div>
    </div>
